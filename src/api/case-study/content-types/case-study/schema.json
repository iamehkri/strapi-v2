{
  "kind": "collectionType",
  "collectionName": "case_studies",
  "info": {
    "singularName": "case-study",
    "pluralName": "case-studies",
    "displayName": "Case Study",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "excerpt": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "pubDate": {
      "type": "date"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "introText": {
      "type": "richtext"
    },
    "bodyText": {
      "type": "richtext"
    },
    "concText": {
      "type": "richtext"
    },
    "caseIconbox": {
      "type": "component",
      "repeatable": true,
      "component": "shared.icon-box"
    },
    "caseStats": {
      "type": "component",
      "repeatable": true,
      "component": "shared.stat-box"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "relateAgents": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ai-agent.ai-agent",
      "inversedBy": "relateCaseStudies"
    },
    "relateFeature": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::feature.feature",
      "inversedBy": "relateCaseStudy"
    },
    "relateIntegrations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::integration.integration",
      "inversedBy": "relateCaseStudy"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "nextjs": {
      "type": "component",
      "repeatable": false,
      "component": "shared.nextjs-comp"
    },
    "industries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::industry.industry",
      "mappedBy": "case_studies"
    }
  }
}
