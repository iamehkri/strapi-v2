{
  "kind": "collectionType",
  "collectionName": "ai_agents",
  "info": {
    "singularName": "ai-agent",
    "pluralName": "ai-agents",
    "displayName": "AI Agent",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "agentName": {
      "type": "string"
    },
    "agentSegment": {
      "type": "string"
    },
    "agentBlurb": {
      "type": "text"
    },
    "iconBox": {
      "type": "component",
      "repeatable": true,
      "component": "shared.icon-box"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "faq": {
      "type": "component",
      "repeatable": true,
      "component": "shared.faq"
    },
    "stats": {
      "type": "component",
      "repeatable": true,
      "component": "shared.stat-box"
    },
    "priceBox": {
      "type": "component",
      "repeatable": true,
      "component": "shared.price-box"
    },
    "agentBlurb2": {
      "type": "text"
    },
    "agentBlurb3": {
      "type": "text"
    },
    "agentAvatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "relateCaseStudies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::case-study.case-study",
      "mappedBy": "relateAgents"
    },
    "nextjs": {
      "type": "component",
      "repeatable": false,
      "component": "shared.nextjs-comp"
    },
    "slug": {
      "type": "uid"
    }
  }
}
